
Storage {
   Name = "<%=@storage_name%>:<%=@name%>"
   Address = <%=@storage_address%>
   SDPort = <%=@port%>
   Password = "<%=@director_password%>"
<% [@devices].each do | dev | -%>
   Device = "<%=@dev%>"
<%end -%>
   Media Type = <%=@mediatype%>
   Maximum Concurrent Jobs = <%= @concurrent_jobs %>
}
